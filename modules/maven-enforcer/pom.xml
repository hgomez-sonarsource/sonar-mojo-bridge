    <dependency>
      <groupId>de.lgohlke.mojo</groupId>
      <artifactId>versions-maven-plugin</artifactId>
    </dependency>

    <dependency>
      <groupId>org.codehaus.sonar</groupId>
      <artifactId>sonar-testing-harness</artifactId>
      <version>${sonar.version}</version>
      <scope>test</scope>
